#include <iostream>

using namespace std;

string remove_space(char str[]){

      int count = 0;
      
      for(int i=0; str[i]; i++){
      
      if(str[i] != ' ')
      str[count] =  str[i] ;
      
      
      count++;
  }
  
  str[count] = '\0';

  return str;
}

int main() {
  char str[] = "Take you forward";
  cout << removeSpaces(str);

  return 0;
}

